{
    "name": "digitalhub/module-doublecheck",
    "description": "by Thiago Covre - Magento 2 Double Check Price Module",
    "type": "magento2-module",
    "version": "1.0.0",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "php": "~7.4||~8.0",
        "magento/framework": ">=102.0.0",
        "magento/module-backend": ">=102.0.0",
        "magento/module-catalog": ">=102.0.0",
        "magento/module-email": ">=102.0.0",
        "magento/module-ui": ">=102.0.0",
        "magento/module-config": ">=102.0.0",
        "magento/module-sales": ">=102.0.0",
        "magento/module-customer": ">=102.0.0"
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "DigitalHub\\DoubleCheck\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "DigitalHub/DoubleCheck"
            ]
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
