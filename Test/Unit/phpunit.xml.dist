<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="bootstrap.php">
    <testsuite name="DigitalHub_DoubleCheck_Test_Unit">
        <directory>./</directory>
    </testsuite>
    <filter>
        <whitelist>
            <directory suffix=".php">../Unit</directory>
        </whitelist>
    </filter>
    <php>
        <ini name="error_reporting" value="-1" />
        <server name="MAGE_MODE" value="developer" />
        <env name="PHPUNIT_TESTS" value="1" />
        <env name="TMP_PATH" value="./tmp" />
    </php>
</phpunit>
